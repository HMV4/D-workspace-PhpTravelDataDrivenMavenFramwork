
public class fibonacci {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int k=0;
		int a,b;
		System.out.print("1 1");
		
		while(k<=50)
		{
			a=1;
			b=1;
			k=a+b;
			System.out.print(" "+k + "  ");
			//System.out.println(" ");
			a=b;
			b=k;
			
			
		}
		
	}

}
