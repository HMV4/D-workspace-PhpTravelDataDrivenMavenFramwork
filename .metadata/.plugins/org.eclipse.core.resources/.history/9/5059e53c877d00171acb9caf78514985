package inside;

public class primenumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		boolean isprime=true;
		
		int num =29;
				
		for(int i=2;i<num;i++)
		{
			if(num%i==0)
			{
				
				
				isprime=false;
				break;
				
			}
			
			
			
			if(isprime)
			{
				System.out.println(" prime num");
					
				
			}
			
			else{System.out.println("Not Prime");}
		}

	}

}
